{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Build SDL2 (MSVC)",
        "type": "shell",
        "command": "cl.exe",
        "args": [
          "main.cpp",
          "SDL2.lib",
          "SDL2main.lib",
          "/I",
          "SDL2\\include",
          "/link",
          "/LIBPATH:SDL2\\lib\\x64",
          "/SUBSYSTEM:WINDOWS",
          "/OUT:main.exe"
        ],
        "options": {
          "cwd": "${workspaceFolder}/practice/practice12"
        },
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": []
      }
    ]
  }
  